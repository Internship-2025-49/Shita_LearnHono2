generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String   @db.VarChar(255)
  content   String?

  @@map("posts")
}

model Auth {
  key       String   @id 

 Â @@map("auth")
}

model Food {
  id        Int      @id @default(autoincrement())
  name     String   @db.VarChar(255)
  theOriginOfFood   String @db.VarChar(255)
  flavour String @db.VarChar(255)
  
  @@map("food")
}